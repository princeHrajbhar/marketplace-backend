  "description": "Production-level Micro Marketplace with Refresh/Access Tokens, RabbitMQ, Google OAuth",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
